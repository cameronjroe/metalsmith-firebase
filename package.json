{
  "name": "metalsmith-firebase",
  "version": "0.6.3",
  "description": "Load firebase data into metalsmith.",
  "main": "lib/index.js",
  "babel": {
    "presets": [
      "latest",
      "stage-0"
    ]
  },
  "scripts": {
    "test": "mocha --compilers js:babel-register",
    "test:watch": "npm t -- -w",
