  "dependencies": {
    "axios": "0.14.0",
    "babel-preset-es2015": "6.14.0",
    "lodash": "4.15.0",
    "metalsmith": "2.2.0",
    "q": "^1.4.1"
  },
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-preset-latest": "6.16.0",
    "babel-preset-stage-0": "6.5.0",
    "chai": "^3.4.1",
    "codecov": "1.0.1",
    "metalsmith": "^2.1.0",
    "nock": "^8.0.0",
    "nyc": "8.1.0"
  }
}
